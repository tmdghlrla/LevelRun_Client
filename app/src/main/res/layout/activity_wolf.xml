<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView29"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="20dp"
        android:gravity="center"
        android:text="컬렉션"
        android:textColor="#000000"
        android:textSize="50sp"
        android:textStyle="bold" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imgWolf1"
            android:layout_width="150dp"
            android:layout_height="180dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:scaleType="fitXY"
            android:src="@drawable/s_wolf_1" />

        <ImageView
            android:id="@+id/imgWolf2"
            android:layout_width="150dp"
            android:layout_height="180dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:scaleType="fitXY"
            android:src="@drawable/s_wolf_2" />

        <ImageView
            android:id="@+id/imgWolf3"
            android:layout_width="150dp"
            android:layout_height="180dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:scaleType="fitXY"
            android:src="@drawable/s_wolf_3" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imgWolf4"
            android:layout_width="150dp"
            android:layout_height="180dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:scaleType="fitXY"
            android:src="@drawable/s_wolf_4" />

        <ImageView
            android:id="@+id/imgWolf5"
            android:layout_width="150dp"
            android:layout_height="180dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:scaleType="fitXY"
            android:src="@drawable/s_wolf_5" />

        <ImageView
            android:id="@+id/imgWolf6"
            android:layout_width="150dp"
            android:layout_height="180dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:scaleType="fitXY"
            android:src="@drawable/s_wolf_6" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imgWolf7"
            android:layout_width="150dp"
            android:layout_height="180dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:scaleType="fitXY"
            android:src="@drawable/s_wolf_7" />

        <ImageView
            android:id="@+id/imgWolf8"
            android:layout_width="150dp"
            android:layout_height="180dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:scaleType="fitXY"
            android:src="@drawable/s_wolf_8" />

        <ImageView
            android:id="@+id/imgWolf9"
            android:layout_width="150dp"
            android:layout_height="180dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:scaleType="fitXY"
            android:src="@drawable/s_wolf_9" />
    </LinearLayout>

</LinearLayout>